{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-underscore-dangle": 0,
    "react/forbid-prop-types": 0,
    "no-debugger": 0,
    "eqeqeq": 0,
    "react/prop-types": 2,
    "global-require": 0,
    "max-len": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "browser": true,
    "document": true,
    "dispatch": true
  }
}
